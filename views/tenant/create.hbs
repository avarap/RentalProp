<main class="w-100 m-auto">
  <div class="container text-start">

    <h4 class="fw-bold">Add new tenant</h4>
    <div class="container p-4 border shadow-sm rounded bg-white m-auto text-start" >

      <form class="row row-cols-lg-auto g-3 align-items-center" action="/tenant/create" method="post" enctype="multipart/form-data" >
        {{> tenantpartial}}
        <div class="col-lg-12">
            <label>
              <span class="hint-text">Select property</span>
            </label>
          <select name="property" class="form-select" multiple>
            <option value="">&nbsp;</option>
            {{!-- <option value="none" selected disabled hidden>Select an Option</option> --}}

            {{#each properties}}
              {{log this._id}}
              <option value="{{this._id}}">{{this.propertyType}} - {{this.referenceID}}</option>
              {{log this.referenceID}}
            {{/each}}
          </select>
        </div>

        <div>
          <input
            class="col btn border-dark bg-white"
            type="reset"
            value="Reset fields"
          />
          <button
            class="col btn btn-secondary border-dark bg-dark"
            type="submit"
          >Create</button>
        </div>
      </form>
    </div>
  </div>
</main>